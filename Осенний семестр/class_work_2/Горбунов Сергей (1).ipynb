    "import random \n",
    "from random import randint\n",
    "def is_prime(a):\n",
    "    i=2\n",
    "    h=0\n",
    "    while(i<a):\n",
    "        i=i+1\n",
    "        if(a%i==0):\n",
    "            h=1\n",
    "    if(h==0):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "v=randint(0,1000)\n",
    "print(is_prime(v))"
   ]
