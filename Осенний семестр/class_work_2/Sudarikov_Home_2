a = 'Привет мир!'
b = list(a)
for i in range(3,8):
    c = b[i]
    print(c.upper())

№7
for i in range(0,500):
  if i%7==0:
    if i%10==8 or (i//10)%10==8 or (i//100)%10==8:
      print(i)
