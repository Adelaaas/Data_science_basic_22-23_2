      "source": [
        "from numpy import *\n",
        "\n",
        "a = array(list(map(float, input().split())), float)\n",
        "z = array([0]*len(a), float)\n",
        "for i in range(0, len(a)):\n",
        "    if a[i] >= 0:\n",
        "      z[i] = ceil(a[i])\n",
        "    else:\n",
        "      z[i] = floor(a[i])\n",
        "print(z)"
      ]
