        "import numpy as np\n",
        "\n",
        "a = np.array([1, 2, 3], float)\n",
        "b = np.array([6, 3, 7], float)\n",
        "\n",
        "res1 = 0\n",
        "res2 = 0\n",
        "res3 = 0\n",
        "\n",
        "for i in range(2):\n",
        "  res1 += a[i] * b[i]\n",
        "  res2 += a[i]**2\n",
        "  res3 += b[i]**2\n",
        "print(res1/(res2 * res3))"
      ]
    }
