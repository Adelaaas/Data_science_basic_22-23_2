        "from numpy import *\n",
        "\n",
        "\n",
        "A = array(list(map(float, input().split())), float)\n",
        "Z = array([0, 0, 0], int)\n",
        "for i in range(0, 3):\n",
        "    if A[i] == 0:\n",
        "      continue\n",
        "    elif A[i] > 0:\n",
        "      Z[i] = int(A[i]) + 1\n",
        "    else:\n",
        "      Z[i] = int(A[i]) - 1\n",
        "print(Z)"
