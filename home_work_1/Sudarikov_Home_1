№1
a = str(input())
print(a[2])
print(a[-2])
print(a[:5])
print(a[:-2])
print(a[::2])
print(a[1::2])
print(a[::-1])
print(a[::-2])
print(len(a))


№2
a = [int(i) for i in input().split()]
b = 0
for i in range(1, len(a) - 1):
    if a[i + 1] < a[i]  and a[i - 1] < a[i]:
        b=b+1
print(b)

